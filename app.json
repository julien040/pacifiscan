{
  "expo": {
    "name": "Pacifiscan",
    "slug": "pacifiscan",
    "description": "Découvrez que faire de vos déchets",
    "version": "2.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "backgroundColor": "#EFF0FF",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "app.pacifiscan.org",
      "supportsTablet": false,
      "buildNumber": "9",
      "infoPlist": {
        "NSCameraUsageDescription": "Cette application utilise la caméra pour scanner des déchets"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "versionCode": 9,
      "package": "app.pacifiscan.org"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["sentry-expo"],
    "extra": {
      "eas": {
        "projectId": "b46e9670-0fc1-4733-a800-21e9e7b996dc"
      }
    }
  }
}
